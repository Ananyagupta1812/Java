// Code to reverse a string
import java.util.Scanner;
import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int ans= 0;
        int m=0;
        while(num>0){
            m=num%10;
            ans=(ans*10)+m;
            num=num/10;
        }
        System.out.print(ans);
    }
}
